<!DOCTYPE html>
<html lang="ja">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.11.2/css/all.css">
    <!-- Google Fonts Roboto -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700&display=swap">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/mdbootstrap/4.7.6/css/mdb.min.css" />
    <title>スプレッドシート登録</title>
</head>
<body>
    <div class="col-md-6 mb-4 pb-5">
        <br>
        <div class="card">
            <div class="card-body">
                <!--Header-->
                <div class="text-center cyan-text">
                    <h3>スプレッドシート登録<!-- <i class="fas fa-file-upload cyan-text"></i>--></h3>
                    <hr class="my-4">
                </div>

                <% if (typeof err_msg === 'undefined') { var err_msg = ''; } %>
                <div class="text-center"><p style="color: red; font-weight: bold;"><%= err_msg %></p></div>

                <!--Body-->
                <div class="md-form">
                    <!-- <i class="fas fa-file-excel prefix grey-text"></i> -->
                    <i class="fas fa-file-upload prefix grey-text"></i>
                    <input type="text" id="sheet_url" name="sheet_url" class="form-control" value="">
                    <label for="sheet_url">マスタファイルURL</label>
                </div>
                <div style="text-align: right;">
                    <!-- <p style="font-size: 90%;"><i class="far fa-folder-open"></i> <a href="googledrive://">Google Driveを開く</a></p> -->
                </div>
                <div class="text-center">
                    <button class="btn btn-info" id="sendmessagebutton">登録</button>
                </div>
            </div>

            <!--Footer-->
<!-- 
            <div class="modal-footer">
            </div>
 -->
        </div>
    </div>

    <script src="https://static.line-scdn.net/liff/edge/2/sdk.js"></script>
    <script src="https://code.jquery.com/jquery-3.4.1.min.js" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/mdbootstrap/4.7.6/js/mdb.min.js"></script>
    <script src="/regist_master.js"></script>
</body>
</html>
